{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "linear-agent-demo",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-12",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "kv_namespaces": [
    {
      "binding": "LINEAR_TOKENS",
      "id": "<your-kv-namespace-id>",
    },
  ],
  "observability": {
    "enabled": true,
  },
  "vars": {
    "URL": "https://linear-agent-demo.<your-subdomain>.workers.dev",
    "LINEAR_CLIENT_ID": "<your-linear-client-id>",
  }
}